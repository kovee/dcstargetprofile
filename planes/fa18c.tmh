		// ---- LEFT - OVER --------------------------------------------------

		// UP Layer
		BTN_EFLOVER_UP_IN			= 0;
		BTN_EFLOVER_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EFLOVER_MIDDLE_IN		= 0;
		BTN_EFLOVER_MIDDLE_OUT		= 0;

		// ---- RIGHT - NORM -------------------------------------------------

		// UP Layer
		BTN_EFRNORM_UP_IN			= 0;
		BTN_EFRNORM_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EFRNORM_MIDDLE_IN		= 0;
		BTN_EFRNORM_MIDDLE_OUT		= 0;

		// ---- RIGHT - OVER -------------------------------------------------

		// UP Layer
		BTN_EFROVER_UP_IN			= 0;
		BTN_EFROVER_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EFROVER_MIDDLE_IN		= 0;
		BTN_EFROVER_MIDDLE_OUT		= 0;

		// **** ENGINE OPERATE SWITCHES **************************************

		// ---- LEFT - IGN ---------------------------------------------------

		// UP Layer
		BTN_EOLIGN_UP_IN			= 0;
		BTN_EOLIGN_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EOLIGN_MIDDLE_IN		= 0;
		BTN_EOLIGN_MIDDLE_OUT		= 0;

		// ---- LEFT - NORM --------------------------------------------------

		// UP Layer
		BTN_EOLNORM_UP_IN			= 0;
		BTN_EOLNORM_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EOLNORM_MIDDLE_IN		= 0;
		BTN_EOLNORM_MIDDLE_OUT		= 0;

		// ---- LEFT - MOTOR -------------------------------------------------

		// UP Layer
		BTN_EOLMOTOR_UP_IN			= 0;
		BTN_EOLMOTOR_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EOLMOTOR_MIDDLE_IN		= 0;
		BTN_EOLMOTOR_MIDDLE_OUT		= 0;

		// ---- RIGHT - IGN --------------------------------------------------

		// UP Layer
		BTN_EORIGN_UP_IN			= 0;
		BTN_EORIGN_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EORIGN_MIDDLE_IN		= 0;
		BTN_EORIGN_MIDDLE_OUT		= 0;

		// ---- RIGHT - NORM -------------------------------------------------

		// UP Layer
		BTN_EORNORM_UP_IN			= 0;
		BTN_EORNORM_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EORNORM_MIDDLE_IN		= 0;
		BTN_EORNORM_MIDDLE_OUT		= 0;

		// ---- RIGHT - MOTOR ------------------------------------------------

		// UP Layer
		BTN_EORMOTOR_UP_IN			= 0;
		BTN_EORMOTOR_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EORMOTOR_MIDDLE_IN		= 0;
		BTN_EORMOTOR_MIDDLE_OUT		= 0;

		// **** APU SWITCH ***************************************************

		// ---- ON -----------------------------------------------------------

		// UP Layer
		BTN_APUON_UP_IN				= 0;
		BTN_APUON_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_APUON_MIDDLE_IN			= 0;
		BTN_APUON_MIDDLE_OUT		= 0;

		// ---- OFF -----------------------------------------------------------

		// UP Layer
		BTN_APUOFF_UP_IN			= 0;
		BTN_APUOFF_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_APUOFF_MIDDLE_IN		= 0;
		BTN_APUOFF_MIDDLE_OUT		= 0;

		// **** FLAPS SWITCH *************************************************

		// ---- UP -----------------------------------------------------------

		// UP Layer
		BTN_FLAPU_UP_IN				= 0;
		BTN_FLAPU_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_FLAPU_MIDDLE_IN			= 0;
		BTN_FLAPU_MIDDLE_OUT		= 0;

		// ---- MIDDLE -------------------------------------------------------

		// UP Layer
		BTN_FLAPM_UP_IN				= 0;
		BTN_FLAPM_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_FLAPM_MIDDLE_IN			= 0;
		BTN_FLAPM_MIDDLE_OUT		= 0;

		// ---- DOWN ---------------------------------------------------------

		// UP Layer
		BTN_FLAPD_UP_IN				= 0;
		BTN_FLAPD_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_FLAPD_MIDDLE_IN			= 0;
		BTN_FLAPD_MIDDLE_OUT		= 0;

		// **** LANDING GEAR HORN BUTTON *************************************

		// UP Layer
		BTN_LDGH_UP_IN				= 0;
		BTN_LDGH_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_LDGH_MIDDLE_IN			= 0;
		BTN_LDGH_MIDDLE_OUT			= 0;

		// **** EAC SWITCH ***************************************************

		// ---- ON -----------------------------------------------------------

		// UP Layer
		BTN_EACON_UP_IN				= 0;
		BTN_EACON_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EACON_MIDDLE_IN			= 0;
		BTN_EACON_MIDDLE_OUT		= 0;

		// ---- OFF ----------------------------------------------------------

		// UP Layer
		BTN_EACOFF_UP_IN			= 0;
		BTN_EACOFF_UP_OUT			= 0;

		// MIDDLE Layer
		BTN_EACOFF_MIDDLE_IN		= 0;
		BTN_EACOFF_MIDDLE_OUT		= 0;

		// **** RDR SWITCH ***************************************************

		// ---- NRM ----------------------------------------------------------

