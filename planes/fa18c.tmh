// Contains control mapping definitions for the F/A-18C Hornet.


int SpecifyFA18ControlMap()
{
	debug("Setting control map for the F/A-18C Hornet.");

	if(JOYSTICK_TYPE == JOYSTICK_TYPE_WARTHOG)
	{
		if(JOYSTICK_GRIP == JOYSTICK_GRIP_A10)
		{
			// ===============================================================
			//	Warthog A-10 Grip
			// ===============================================================

			// **** TRIGGER - STAGE 1 ****************************************

			// UP Layer
			BTN_TG1_UP_IN				= 0;
			BTN_TG1_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_TG1_MIDDLE_IN			= 0;
			BTN_TG1_MIDDLE_OUT			= 0;

			// **** TRIGGER - STAGE 2 ****************************************

			// UP Layer
			BTN_TG2_UP_IN				= 0;
			BTN_TG2_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_TG2_MIDDLE_IN			= 0;
			BTN_TG2_MIDDLE_OUT			= USB_SPACE; // Gun Trigger

			// **** S1 BUTTON ************************************************

			// UP Layer
			BTN_S1_UP_IN				= 0;
			BTN_S1_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_S1_MIDDLE_IN			= 0;
			BTN_S1_MIDDLE_OUT			= 0;

			// **** S2 BUTTON ************************************************

			// UP Layer
			BTN_S2_UP_IN				= 0;
			BTN_S2_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_S2_MIDDLE_IN			= 0;
			BTN_S2_MIDDLE_OUT			= R_ALT+USB_SPACE; // Weapon Release

			// **** S3 BUTTON ************************************************

			// UP Layer
			BTN_S3_UP_IN				= 0;
			BTN_S3_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_S3_MIDDLE_IN			= USB_A; // Autopilot/Nosewheel
												 // steering disengage
			BTN_S3_MIDDLE_OUT			= USB_S; // Undesignate/Nosewheel
												 // steering

			// **** TRIM HAT *************************************************

			// ---- UP -------------------------------------------------------

			// UP Layer
			BTN_H1U_UP_IN				= 0;
			BTN_H1U_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H1U_MIDDLE_IN			= 0;
			BTN_H1U_MIDDLE_OUT			= R_CTL+USB_SEMICOLON; // Nose Down

			// ---- DOWN -----------------------------------------------------

			// UP Layer
			BTN_H1D_UP_IN				= 0;
			BTN_H1D_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H1D_MIDDLE_IN			= 0;
			BTN_H1D_MIDDLE_OUT			= R_CTL+USB_PERIOD; // Nose Up

			// ---- LEFT -----------------------------------------------------

			// UP Layer
			BTN_H1L_UP_IN				= 0;
			BTN_H1L_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H1L_MIDDLE_IN			= 0;
			BTN_H1L_MIDDLE_OUT			= R_CTL+USB_COMMA; // Left Wing Down

			// ---- RIGHT ----------------------------------------------------

			// UP Layer
			BTN_H1R_UP_IN				= 0;
			BTN_H1R_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H1R_MIDDLE_IN			= 0;
			BTN_H1R_MIDDLE_OUT			= R_CTL+USB_SLASH; // Right Wing Down

			// **** TMS HAT **************************************************

			// ---- UP -------------------------------------------------------

			// UP Layer
			BTN_H2U_UP_IN				= 0;
			BTN_H2U_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H2U_MIDDLE_IN			= 0;
			BTN_H2U_MIDDLE_OUT			= L_SHIFT+USB_X; // Select Gun

			// ---- DOWN -----------------------------------------------------

			// UP Layer
			BTN_H2D_UP_IN				= 0;
			BTN_H2D_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H2D_MIDDLE_IN			= 0;
			BTN_H2D_MIDDLE_OUT			= L_SHIFT+USB_W ; // Select Sparrow

			// ---- LEFT -----------------------------------------------------

			// UP Layer
			BTN_H2L_UP_IN				= 0;
			BTN_H2L_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H2L_MIDDLE_IN			= 0;
			BTN_H2L_MIDDLE_OUT			= L_SHIFT+USB_S; // Select Sidewinder

			// ---- RIGHT ----------------------------------------------------

			// UP Layer
			BTN_H2R_UP_IN				= 0;
			BTN_H2R_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H2R_MIDDLE_IN			= 0;
			BTN_H2R_MIDDLE_OUT			= L_SHIFT+USB_D; // Select AMRAAM

			// **** DMS HAT **************************************************

			// ---- UP -------------------------------------------------------

			// UP Layer
			BTN_H3U_UP_IN				= 0;
			BTN_H3U_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H3U_MIDDLE_IN			= 0;
			BTN_H3U_MIDDLE_OUT			= USB_E; // Chaff

			// ---- DOWN -----------------------------------------------------

			// UP Layer
			BTN_H3D_UP_IN				= 0;
			BTN_H3D_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H3D_MIDDLE_IN			= 0; // Dispenser Switch Off
			BTN_H3D_MIDDLE_OUT			= USB_D; // Flare

			// ---- LEFT -----------------------------------------------------

			// UP Layer
			BTN_H3L_UP_IN				= 0;
			BTN_H3L_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H3L_MIDDLE_IN			= 0; // Dispenser Switch Bypass
			BTN_H3L_MIDDLE_OUT			= 0;

			// ---- RIGHT ----------------------------------------------------

			// UP Layer
			BTN_H3R_UP_IN				= 0;
			BTN_H3R_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H3R_MIDDLE_IN			= 0; // Dispenser Switch On
			BTN_H3R_MIDDLE_OUT			= 0;

			// **** CMS HAT **************************************************

			// ---- UP -------------------------------------------------------

			// UP Layer
			BTN_H4U_UP_IN				= 0;
			BTN_H4U_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H4U_MIDDLE_IN			= 0;
			BTN_H4U_MIDDLE_OUT			= R_ALT+USB_SEMICOLON  ; // SCS FWD

			// ---- DOWN -----------------------------------------------------

			// UP Layer
			BTN_H4D_UP_IN				= 0;
			BTN_H4D_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H4D_MIDDLE_IN			= 0;
			BTN_H4D_MIDDLE_OUT			= R_ALT+USB_PERIOD ; // SCS AFT

			// ---- LEFT -----------------------------------------------------

			// UP Layer
			BTN_H4L_UP_IN				= 0;
			BTN_H4L_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H4L_MIDDLE_IN			= 0;
			BTN_H4L_MIDDLE_OUT			= R_ALT+USB_COMMA; // SCS LEFT

			// ---- RIGHT ----------------------------------------------------

			// UP Layer
			BTN_H4R_UP_IN				= 0;
			BTN_H4R_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H4R_MIDDLE_IN			= 0;
			BTN_H4R_MIDDLE_OUT			= R_ALT+USB_SLASH; // SCS RIGHT

			// ---- PRESS ----------------------------------------------------

			// UP Layer
			BTN_H4P_UP_IN				= 0;
			BTN_H4P_UP_OUT				= 0;

			// MIDDLE Layer
			BTN_H4P_MIDDLE_IN			= 0;
			BTN_H4P_MIDDLE_OUT			= 0; // SCS Depress

		}
		else if (JOYSTICK_GRIP == JOYSTICK_GRIP_FA18)
		{
			// ===============================================================
			//	Warthog F/A-18 Grip
			// ===============================================================
		}
		else if (JOYSTICK_GRIP == JOYSTICK_GRIP_COUGAR)
		{
			// ===============================================================
			//	Warthog Cougar Grip
			// ===============================================================
		}
		else
		{
			error("Unsupported Warthog grip type.");
		}
	}
	else
	{
		error("Unsupported joystick type.");
	}

	if(THROTTLE_TYPE == THROTTLE_TYPE_WARTHOG)
	{
		// ===================================================================
		//	Warthog Throttle Grip
		// ===================================================================

		// **** MIC SWITCH ***************************************************

		// ---- UP -----------------------------------------------------------

		// UP Layer
		BTN_MSU_UP_IN				= 0;
		BTN_MSU_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_MSU_MIDDLE_IN			= USB_BACKSLASH; // Communications Menu
		BTN_MSU_MIDDLE_OUT			= 0;

		// ---- DOWN ---------------------------------------------------------

		// UP Layer
		BTN_MSD_UP_IN				= 0;
		BTN_MSD_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_MSD_MIDDLE_IN			= L_SHIFT+USB_BACKSLASH // Switch Dialog
		BTN_MSD_MIDDLE_OUT			= 0;

		// ---- LEFT ---------------------------------------------------------

		// UP Layer
		BTN_MSL_UP_IN				= 0;
		BTN_MSL_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_MSL_MIDDLE_IN			= 0;
		BTN_MSL_MIDDLE_OUT			= USB_2; // COMM 2

		// ---- RIGHT --------------------------------------------------------

		// UP Layer
		BTN_MSR_UP_IN				= 0;
		BTN_MSR_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_MSR_MIDDLE_IN			= 0;
		BTN_MSR_MIDDLE_OUT			= USB_1; // COMM 1

		// ---- PRESS --------------------------------------------------------

		// UP Layer
		BTN_MSP_UP_IN				= 0;
		BTN_MSP_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_MSP_MIDDLE_IN			= 0;
		BTN_MSP_MIDDLE_OUT			= 0;

		// **** SPEEDBRAKE SWITCH ********************************************

		// ---- BACK ---------------------------------------------------------

		// UP Layer
		BTN_SPDB_UP_IN				= 0;
		BTN_SPDB_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_SPDB_MIDDLE_IN			= 0;
		BTN_SPDB_MIDDLE_OUT			= L_SHIFT+USB_B ; // Speedbrake extend

		// ---- MIDDLE -------------------------------------------------------

		// UP Layer
		BTN_SPDM_UP_IN				= 0;
		BTN_SPDM_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_SPDM_MIDDLE_IN			= 0;
		BTN_SPDM_MIDDLE_OUT			= 0;

		// ---- FRONT --------------------------------------------------------

		// UP Layer
		BTN_SPDF_UP_IN				= 0;
		BTN_SPDF_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_SPDF_MIDDLE_IN			= 0;
		BTN_SPDF_MIDDLE_OUT			= L_CTL+USB_B; // Speedbrake retract

		// **** BOAT SWITCH **************************************************

		// ---- BACK ---------------------------------------------------------

		// UP Layer
		BTN_BSB_UP_IN				= 0;
		BTN_BSB_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_BSB_MIDDLE_IN			= 0;
		BTN_BSB_MIDDLE_OUT			= 0;

		// ---- MIDDLE -------------------------------------------------------

		// UP Layer
		BTN_BSM_UP_IN				= 0;
		BTN_BSM_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_BSM_MIDDLE_IN			= 0;
		BTN_BSM_MIDDLE_OUT			= 0;

		// ---- FRONT --------------------------------------------------------

		// UP Layer
		BTN_BSF_UP_IN				= 0;
		BTN_BSF_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_BSF_MIDDLE_IN			= 0;
		BTN_BSF_MIDDLE_OUT			= 0;

		// **** CHINA HAT ****************************************************

		// ---- BACK ---------------------------------------------------------

		// UP Layer
		BTN_CHB_UP_IN				= 0;
		BTN_CHB_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_CHB_MIDDLE_IN			= 0;
		BTN_CHB_MIDDLE_OUT			= 0;

		// ---- MIDDLE -------------------------------------------------------

		// UP Layer
		BTN_CHM_UP_IN				= 0;
		BTN_CHM_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_CHM_MIDDLE_IN			= 0;
		BTN_CHM_MIDDLE_OUT			= 0;

		// ---- FRONT --------------------------------------------------------

		// UP Layer
		BTN_CHF_UP_IN				= 0;
		BTN_CHF_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_CHF_MIDDLE_IN			= 0;
		BTN_CHF_MIDDLE_OUT			= 0;

		// **** LEFT THROTTLE BUTTON *****************************************

		// UP Layer
		BTN_LTB_UP_IN				= 0;
		BTN_LTB_UP_OUT				= 0;

		// MIDDLE Layer
		BTN_LTB_MIDDLE_IN			= USB_T; // ATC Engage/Disengate
		BTN_LTB_MIDDLE_OUT			= USB_C; // Cage/Uncage

		// **** COOLIE HAT ***************************************************

		// ---- UP -----------------------------------------------------------

		// UP Layer
		BTN_CSU_UP_IN				= 0;
		BTN_CSU_UP_OUT				= 0;

		// MIDDLE Layer
		if(DCS_MODE == DCS_MODE_VR)
		{
			BTN_CSU_MIDDLE_IN		= 0;
			BTN_CSU_MIDDLE_OUT		= R_SHIFT+USB_K; // Kneeboard On/Off
		}
		else
		{
			BTN_CSU_MIDDLE_IN		= R_SHIFT+USB_K; // Kneeboard On/Off
			BTN_CSU_MIDDLE_OUT		= USB_NUMPAD_8; // View Up
		}

		// ---- DOWN ---------------------------------------------------------

		// UP Layer
		BTN_CSD_UP_IN				= 0;
		BTN_CSD_UP_OUT				= 0;

		// MIDDLE Layer
		if(DCS_MODE == DCS_MODE_VR)
		{
			BTN_CSD_MIDDLE_IN		= 0;
			BTN_CSD_MIDDLE_OUT		= 0;
		}
		else
		{
			BTN_CSD_MIDDLE_IN		= USB_NUMPAD_5; // View Center
			BTN_CSD_MIDDLE_OUT		= USB_NUMPAD_2; // View Down
		}

		// ---- LEFT ---------------------------------------------------------

		// UP Layer
		BTN_CSL_UP_IN				= 0;
		BTN_CSL_UP_OUT				= 0;

		// MIDDLE Layer
		if(DCS_MODE == DCS_MODE_VR)
		{
